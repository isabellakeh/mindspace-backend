# Server Configuration
NODE_ENV=production
PORT=3000
CORS_ORIGIN=cors

# Azure MySQL Database
AZURE_MYSQL_HOST=server.mysql.database.azure.com
AZURE_MYSQL_USER=usernmame
AZURE_MYSQL_PASSWORD=password
AZURE_MYSQL_DATABASE=platform
AZURE_MYSQL_PORT=3306
AZURE_MYSQL_SSL_CA=-

# Azure Cosmos DB (MongoDB API)
COSMOS_DB_CONNECTION_STRING=-
COSMOS_DB_NAME=database

# JWT Configuration
JWT_SECRET=secret-key
JWT_REFRESH_SECRET=secret-key-2

# NVIDIA NIMs Configuration
NVIDIA_API_KEY=-

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=combined

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000